ImagePost.styles = <%= ImagePost::Style.all.to_json %>;

$.each(ImagePost.styles, function(index, style){
  if (!style.backgroundImageUrl) return;
  var image = new Image
  image.src = style.backgroundImageUrl;
  style.backgroundImage = image;
});

ImagePost.randomStyleIndex = function() {
  return Math.floor(Math.random()*ImagePost.styles.length);
};
